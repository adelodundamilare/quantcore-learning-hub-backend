# Add these to your .env file for Redis caching

# Redis Configuration (optional - will fall back to memory cache)
REDIS_URL=redis://localhost:6379/0

CACHE_TTL=300  # Default cache TTL in seconds (5 minutes)

SECRET_KEY=your-secret-key-here
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=your-db-user
DATABASE_PASSWORD=your-db-password
DATABASE_NAME=your-db-name
SENDGRID_API_KEY=your-sendgrid-key
EMAILS_FROM_EMAIL=noreply@yourapp.com
EMAILS_FROM_NAME=Your App Name
CLOUDINARY_CLOUD_NAME=your-cloudinary-name
CLOUDINARY_API_KEY=your-cloudinary-key
CLOUDINARY_API_SECRET=your-cloudinary-secret
POLYGON_API_KEY=your-polygon-key
STRIPE_SECRET_KEY=your-stripe-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret